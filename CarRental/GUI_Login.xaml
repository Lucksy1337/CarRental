<Window x:Class="CarRental.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarRental"
        mc:Ignorable="d"
        Title="Anmeldefenster" Height="150.228" Width="246.113" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="buttonLogin" Content="Anmelden" HorizontalAlignment="Left" Margin="60,89,0,0" VerticalAlignment="Top" Width="122" FontSize="14" Click="buttonLogin_Click"/>
        <TextBox x:Name="textBoxUsername" HorizontalAlignment="Left" Height="23" Margin="114,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="112"/>
        <Label x:Name="labelUsername" Content="Benutzername" HorizontalAlignment="Left" Margin="13,11,0,0" VerticalAlignment="Top" Width="100" FontSize="14"/>
        <Label x:Name="labelPassword" Content="Passwort" HorizontalAlignment="Left" Margin="13,49,0,0" VerticalAlignment="Top" Width="91" FontSize="14"/>
        <PasswordBox x:Name="passwordBoxPassword" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="114,52,0,0" Width="112" Background="White" Height="23"/>
    </Grid>
</Window>
